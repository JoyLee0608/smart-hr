{
  "rewrites": [
    {
      "source": "/api/proxy/(.*)",
      "destination": "https://hiagent.volcenginepaas.com/api/proxy/api/v1/$1",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "POST, GET, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization, Apikey"
      }
    }
  ]
}